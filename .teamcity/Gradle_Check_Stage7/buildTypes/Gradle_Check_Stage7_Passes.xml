<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="ab5607c6-62c9-47e8-96b7-01036c264cf9" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Stage 7 Passes</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="build/build-receipt.properties" />
      <option name="checkoutMode" value="ON_AGENT" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="GRADLE_RUNNER" type="gradle-runner">
        <parameters>
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRUnner.gradle.build.file" value="build.gradle" />
          <param name="ui.gradleRunner.additional.gradle.cmd.params" value="-PtimestampedVersion -Djava7.home=%linux.jdk.for.gradle.compile%" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="createBuildReceipt" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="RUNNER_165" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[REPO=/home/%env.USER%/.m2/repository
if [ -e $REPO ] ; then
echo "$REPO was polluted during the build"
return -1
else
echo "$REPO does not exist"
fi]]></param>
          <param name="teamcity.step.mode" value="execute_always" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="CHECK_CLEAN_M2" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[REPO=/home/%env.USER%/.m2/repository
if [ -e $REPO ] ; then
tree $REPO
rm -rf $REPO
echo "$REPO was polluted during the build"
return 1
else
echo "$REPO does not exist"
fi]]></param>
          <param name="teamcity.step.mode" value="execute_always" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Gradle_Branches_GradlePersonalBranches" />
    </vcs-settings>
    <requirements>
      <contains id="RQ_1" name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="schedulingTrigger">
        <parameters>
          <param name="branchFilter" value="+:master" />
          <param name="cronExpression_dm" value="*" />
          <param name="cronExpression_dw" value="?" />
          <param name="cronExpression_hour" value="*" />
          <param name="cronExpression_min" value="0" />
          <param name="cronExpression_month" value="*" />
          <param name="cronExpression_sec" value="0" />
          <param name="cronExpression_year" value="*" />
          <param name="dayOfWeek" value="Saturday" />
          <param name="enableQueueOptimization" value="true" />
          <param name="hour" value="1" />
          <param name="minute" value="0" />
          <param name="promoteWatchedBuild" value="true" />
          <param name="revisionRule" value="lastFinished" />
          <param name="revisionRuleBuildBranch" value="&lt;default&gt;" />
          <param name="schedulingPolicy" value="weekly" />
          <param name="timezone" value="SERVER" />
          <param name="triggerBuildIfWatchedBuildChanges" value="" />
          <param name="triggerBuildWithPendingChangesOnly" value="true" />
          <param name="triggerRules"><![CDATA[-:design-docs
-:subprojects/docs/src/docs/release]]></param>
        </parameters>
      </build-trigger>
    </build-triggers>
    <dependencies>
      <depend-on sourceBuildTypeId="Gradle_Check_PerformanceHistoricalBuild">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Check_Stage6_Passes">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Check_TestCoverageCrossVersionFullJava7Linux_1">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Check_TestCoverageCrossVersionFullJava7Windows_1">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Check_TestCoverageNoDaemonJava8Linux_1">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Check_TestCoverageNoDaemonJava8Windows_1">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

