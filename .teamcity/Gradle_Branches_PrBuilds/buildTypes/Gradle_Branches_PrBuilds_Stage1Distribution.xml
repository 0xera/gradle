<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="8bbb39d7-134a-4fd8-ad17-746f4dcc030d" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Stage 1 - Distribution</name>
  <description>Can be built as a production distribution</description>
  <settings>
    <options>
      <option name="artifactRules" value="build/build-receipt.properties" />
      <option name="checkoutMode" value="ON_AGENT" />
    </options>
    <disabled-settings>
      <setting-ref ref="vcsTrigger" />
    </disabled-settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="GRADLE_RUNNER" type="gradle-runner">
        <parameters>
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRUnner.gradle.build.file" value="build.gradle" />
          <param name="ui.gradleRunner.additional.gradle.cmd.params" value="-PtimestampedVersion" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="createBuildReceipt" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="CHECK_CLEAN_M2" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[REPO=/home/%env.USER%/.m2/repository
if [ -e $REPO ] ; then
rm -rf $REPO
echo "$REPO was polluted during the build"
return 1
else
echo "$REPO does not exist"
fi]]></param>
          <param name="teamcity.step.mode" value="execute_always" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Gradle_Branches_GradleInternalPRs" />
    </vcs-settings>
    <requirements>
      <contains id="RQ_1" name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter"><![CDATA[+:pull/*
-:<default>]]></param>
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="USE_DEFAULT" />
          <param name="triggerRules"><![CDATA[+:user=adam@gradle.com:**
+:user=sterling@gradle.com:**
+:user=bigdaz:**
+:user=eric@gradle.com:**
+:user=eriwen:**
+:user=eskatos:**
+:user=gary:**
+:user=lacasseio:**
+:user=lptr:**
+:user=oehme@gradle.com:**
+:user=cedric@gradle.com:**
+:user=wolf@gradle.com:**]]></param>
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_28" type="commit-status-publisher">
        <parameters>
          <param name="github_authentication_type" value="token" />
          <param name="github_host" value="https://api.github.com" />
          <param name="publisherId" value="githubStatusPublisher" />
          <param name="secure:github_access_token" value="credentialsJSON:0a13a659-4588-4122-937c-b740a2d2ba17" />
          <param name="vcsRootId" value="Gradle_Branches_GradleInternalPRs" />
        </parameters>
      </extension>
    </build-extensions>
    <dependencies>
      <depend-on sourceBuildTypeId="Gradle_Branches_PrBuilds_BuildDistributions">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="run-build-if-dependency-failed-to-start" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Branches_PrBuilds_SanityCheck">
        <options>
          <option name="run-build-if-dependency-failed" value="CANCEL" />
          <option name="run-build-if-dependency-failed-to-start" value="CANCEL" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

