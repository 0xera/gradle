<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="f71651d5-95cf-4312-adc4-b8185f4985ed" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/10.0/project-config.xsd">
  <name>Stage 3 Passes</name>
  <description>Passes devBuild on linux and windows and compatibility builds</description>
  <settings>
    <options>
      <option name="artifactRules" value="build/build-receipt.properties" />
      <option name="checkoutMode" value="ON_AGENT" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="GRADLE_RUNNER" type="gradle-runner">
        <parameters>
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRUnner.gradle.build.file" value="build.gradle" />
          <param name="ui.gradleRunner.additional.gradle.cmd.params" value="-PtimestampedVersion -Djava7.home=%linux.jdk.for.gradle.compile%" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="createBuildReceipt" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="RUNNER_165" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[REPO=/home/%env.USER%/.m2/repository
if [ -e $REPO ] ; then
echo "$REPO was polluted during the build"
return -1
else
echo "$REPO does not exist"
fi]]></param>
          <param name="teamcity.step.mode" value="execute_always" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="CHECK_CLEAN_M2" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[REPO=/home/%env.USER%/.m2/repository
if [ -e $REPO ] ; then
tree $REPO
rm -rf $REPO
echo "$REPO was polluted during the build"
return 1
else
echo "$REPO does not exist"
fi]]></param>
          <param name="teamcity.step.mode" value="execute_always" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Gradle_Branches_GradlePersonalBranches" />
    </vcs-settings>
    <requirements>
      <contains id="RQ_1" name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="Gradle_Check_Stage2_Passes">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="Gradle_Check_TestCoverageEmbeddedJava7Windows_1">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

